<%- contentFor('jumbotron') %>
<div class="jumbotron">
  <div class="container">
    <h1>All About us</h1>
    <p>We'are a small team.</p>
    <p>
      <a href="/contact" class="btn btn-primary btn-lg">Drop us a line</a>
    </p>
  </div>
</div>  

<%- contentFor('body') %>
<div class="row">
  <% for (user of users) { %>
    <div class="col-sm-3">
      <img src="<%= user.avatar %>" />
      <h2><%= user.name %></h2>
      <p><%= user.email %></p>
    </div>
  <% } %>
</div>